[
  {
    "name": "node-exporter",
    "image": "prom/node-exporter",
    "essential": true,
    "memory": 128,
    "portMappings": [
      {
        "containerPort": 9100,
        "hostPort": 9100,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "ex-r53-auto-naming-awslogs-ecs",
            "awslogs-region": "ap-northeast-1",
            "awslogs-stream-prefix": "node-exporter"
        }
    }
  }
]
